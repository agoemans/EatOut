<!-- templates/homepage.twig -->
<!DOCTYPE html>
<html>
    <head><!-- whatever -->
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('/../views/css/style.css') }}" media="all" />
            <link rel="stylesheet" href="{{ bootstrapPath }}" media="all" />

        {% endblock %}
    </head>
    <body>
        <div class="container">

            <ul id="finalList">
                <h3>Name of restaurants</h3>
                {% for item in finalList %}
                    <li><h3><small>{{ item.placename|striptags|title }}</small></h3></li>
                    <li>
                        <h4>
                            <small>{{ item.streetname }}</small>
                        </h4>
                    </li>
                    <li id = "last-child">
                        <h4>
                            <small>{{ item.zipcode }}</small>
                        </h4>
                    </li>

                    </br>
                {% endfor %}
            </ul>
        </div>
    </body>
</html>